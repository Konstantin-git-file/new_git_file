public class ExampeMassive {
    public static void main(String[] args) {

        int n = 5;
        double [] a = new double[n];
        for (int i = 0; i < n; i++)
            a [i] = Math.random();

        double max = Double.NEGATIVE_INFINITY;
        for (int i = 0; i < n; i++) {
            if (a[i] > max)
                max = a[i];
            System.out.println(a[i]);
            }
        }
    }